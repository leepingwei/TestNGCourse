<!-- Exclude will run all the @Test except the ones with 'SmokeTest' group -->

<suite name = "Suite">
    <test name = "Test">

        <groups>
            <run>
                <exclude name = "SmokeTest"></exclude>
            </run>
        </groups>

        <classes>
            <class name = "Day5._04_SecondGroupsClass"></class>
        </classes>

    </test>
</suite>